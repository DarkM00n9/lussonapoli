<header id="header"> 
  <div class="logo">
    <a href="{{ shop.url }}">
      {% if settings.logo %}
        <img src="{{ settings.logo | image_url: 'large' | image_tag }}" alt="{{ shop.name }}" />
      {% else %}
        <span>No logo available</span>
      {% endif %}
    </a>
  </div>
  <nav>
    <ul>
      <li><a href="/">Development</a></li>
    </ul>
  </nav>
</header>


{%- schema -%}
{
"name": "Header",
"settings": [
  {
  "type": "image_picker",
  "id": "logo",
  "label": "Logo"
  }
]
}
{%- endschema -%}

